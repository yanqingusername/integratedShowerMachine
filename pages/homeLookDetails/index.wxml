
<view class="page">
    <view class="container">
  <ec-canvas id="mychart-dom-line" canvas-id="mychart-line" ec="{{ ec }}"></ec-canvas>
</view>
    <view class="hld_1">
        <view class="hld_1_1">
            <view class="hld_1_1_l" style="padding-left: 20rpx;">
                <view class="hld_1_1_l_1">开始日期</view>
                <view class="hld_1_1_l_2">
                    <picker mode="date" value="{{startDate}}" start="2020-01-01" end="{{yearmouthday}}" bindchange="bindStartDateChange">
                        <view style="display: flex;align-items: center;">
                            <view class="picker">{{startDate}}</view>
                            <image class='hld_1_1_l_2_img' src='/image/icon/icon_2023_01_06_13.png'></image>
                        </view>
                    </picker>
                </view>
            </view>
            <view class="hld_1_1_l">
                <view class="hld_1_1_l_1">结束日期</view>
                <view class="hld_1_1_l_2">
                    <picker mode="date" value="{{endDate}}" start="2020-01-01" end="{{yearmouthday}}" bindchange="bindEndDateChange">
                        <view style="display: flex;align-items: center;">
                            <view class="picker">{{endDate}}</view>
                            <image class='hld_1_1_l_2_img' src='/image/icon/icon_2023_01_06_13.png'></image>
                        </view>
                    </picker>
                </view>
            </view>
        </view>
        <view class="hld_1_2"></view>
        <view class="hld_1_3">
            <view class="hld_1_3_1" style="padding-left: 20rpx;" bind:tap="bingNameHandler">
                <view class="hld_1_3_1_1">姓名</view>
                <text class="hld_1_3_1_2"></text>
            </view>
            <view class="hld_1_3_1" style="justify-content: center;" bind:tap="bingResultHandler">
                <view class="hld_1_3_1_1">结果</view>
                <text class="hld_1_3_1_2"></text>
            </view>
            <view class="hld_1_3_1" style="padding-right: 20rpx;justify-content: flex-end;" bind:tap="bingWorkHandler">
                <view class="hld_1_3_1_1">考勤</view>
                <text class="hld_1_3_1_2"></text>
            </view>

           
             

        </view>

        


    </view>

