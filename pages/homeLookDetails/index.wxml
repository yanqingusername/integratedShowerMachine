                        </view>
                        <view class="homeindex_4_1_item_center">
                            <view class="homeindex_4_1_item_l">
                                <view class="homeindex_4_1_item_l_1">
                                    <image class='homeindex_4_1_item_l_h' src='/image/icon/icon_2023_01_06_09.png'></image>
                                </view>
                                <view class="homeindex_4_1_item_l_2">
                                <view class="homeindex_4_1_item_l_2_title">张章<view class="homeindex_4_1_top_1_l {{index==2 ? 'homeindex_4_1_top_1_l_1':''}}">上班</view></view>
